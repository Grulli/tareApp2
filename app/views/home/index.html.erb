<ul class="breadcrumb">
  <li><a href=<%= home_path %>>Inicio</a> <span class="divider">/</span></li>
</ul>

<h1>Bienvenido a tareApp 2.0 - Una obra en proceso</h1>
<br>
<br>
<% if !session[:user_id] %>
	<% if !session[:login_error_count] %>
		<%= render 'login' %>
	<% else %>
		<% if session[:login_error_count].to_i < 3 %>
			<%= render 'login' %>
		<% else %>
			<%= render 'login_2' %>
		<% end %>
	<% end %>
	<br/>
<% else %>
	<!-- Homeworks partial-->
	<%= render 'homework_index_partial' %>
<% end %>